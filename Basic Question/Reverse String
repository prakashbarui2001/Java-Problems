import java.util.Scanner;
import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef
{
  
      
   
	public static void main (String[] args) throws java.lang.Exception
	{
	     String input = "Geeks for Geeks";
	    int n=input.length();
	    char s[]=input.toCharArray();// This is conver string to character array
	    int low=0;
	    int high=n-1;
	    while(low<high)
	    {
	        char p=s[low];
	        s[low]=s[high];
	        s[high]=p;
	        low++;
	        high--;
	    }
	   String str = String.valueOf(s); // This is conver character array ti the string
	   System.out.println(str);
       
	    // This way i convert string to Character arry;
	    
	    
	}
}
