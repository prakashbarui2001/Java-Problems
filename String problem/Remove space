import java.util.*;
public class Main {
  
  
  public static void main(String[] args) {
    String str = "Prakash Barui";
    
       int n=str.length();
       for(int i=0;i<n;++i){
           
           if(str.charAt(i)==' '){
               continue;
           }
           System.out.print(str.charAt(i));
       }
      
  }
}
