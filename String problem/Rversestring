import java.util.*;
import java.lang.*;
import java.io.*;
import java.util.Scanner;
public class Main
{
    static String isPalindrome(String s)
    {
        int n=s.length();
        String rev="";
        for(int i=0;i<n;++i){
	   ch=s.charAt(i);
	   rev=ch+rev;
	}
        return rev;
      
       
    }
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		System.out.printf(isPalindrome(s));
		
	}
}
